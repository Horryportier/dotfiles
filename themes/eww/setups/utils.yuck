(defpoll date_jp :interval "1h" 
  `date +"「%Y」・「%m ・%d」"`)
(defpoll time_jp :interval "1s" 
  `date +"「%H・%M ・%S」"`)

(defpoll time :interval "1s" 
  `date`)

(defpoll notes :interval "30s" 
  `cat ~/paradox.md`)


(defpoll keys :interval "1s" 
  `setxkbmap -print | grep xkb_symbols | grep -o -P '(?<=pc).*(?=inet)' | cut -d "+" -f 2`)
